@using ProyectoFinal.Models
@model Student

<h2>Add Your Profile</h2>
<form asp-action="AddProfile" method="post">
    <div class="form-group">
    <input asp-for="@Model.UserId" type="hidden" />
    <label asp-for="StudentName"></label>
    <input class="form-control" asp-for="StudentName" />
    <br />
    <label asp-for="StudentDNI"></label>
    <input class="form-control" asp-for="StudentDNI" />
    <br />
    <label asp-for="StudentAge"></label>
    <input class="form-control" asp-for="StudentAge" />
    <br />
    <label asp-for="StudentAddress"></label>
    <input class="form-control" asp-for="StudentAddress" />
    <br />
    <label asp-for="StudentEmail"></label>
    <input class="form-control" asp-for="StudentEmail" />
    <br />
    </div>
    <button class="btn btn-success" type="submit">Add Profile</button>
</form>
<p><a asp-action="Index">Back to Student Home</a></p>