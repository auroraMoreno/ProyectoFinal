@using ProyectoFinal.Models
@model Student

<h2>Add Your Profile</h2>
<form asp-action="AddProfile">
    <input asp-for="@Model.UserId" type="hidden" />
    <label asp-for="StudentName"></label>
    <input asp-for="StudentName" />
    <br />
    <label asp-for="StudentDNI"></label>
    <input asp-for="StudentDNI" />
    <br />
    <label asp-for="StudentAge"></label>
    <input asp-for="StudentAge" />
    <br />
    <label asp-for="StudentAddress"></label>
    <input asp-for="StudentAddress" />
    <br />
    <label asp-for="StudentEmail"></label>
    <input asp-for="StudentEmail" />
    <br />
    <button type="submit">Add Profile</button>
</form>
<p><a asp-action="Index">Back to Student Home</a></p>