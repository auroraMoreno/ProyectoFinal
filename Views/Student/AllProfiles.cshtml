@using ProyectoFinal.Models
@model IEnumerable<Student>

@{
    if (Model.Count() < 1)
    {
        <p>There are no profiles yet</p>
        <p><a asp-controller="Secretary" asp-action="Index">Return</a></p>
    }
    else
    {
        <h3>All profiles</h3>
        <table class="table table-hover">
            <thead class="bg-info">
                <tr>
                    <th>ID</th>
                    <th>Profile Name</th>
                    <th>Office Location</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var i in Model)
                {
                    <tr>
                        <td>@i.StudentId</td>
                        <td>@i.StudentName</td>
                        <td>@i.StudentDNI</td>
                        <td><a>See Payments</a></td>
                    </tr>
                }
            </tbody>
        </table>
    }
}
<p><a asp-controller="Secretary" asp-action="Index">Return</a></p>